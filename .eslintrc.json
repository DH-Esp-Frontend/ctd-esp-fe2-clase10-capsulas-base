{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "max-classes-per-file": "off",
        "no-console": "off",
        "no-unused-vars": "warn",
        "lines-between-class-members": "off",
        "quotes": "warn",
        "linebreak-style": "off",
        "indent": "off",
        "max-len": "off"
    }
}
